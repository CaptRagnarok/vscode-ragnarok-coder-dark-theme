{
	"name": "Ragnarok",
	"displayName":"Ragnarok Coder Dark",
	"version": "0.7.5",
	"publisher": "RagnarokCoding",
	"description": "A yellow/blue theme optimized for HTML / CSS / JS. May continue development for other languages upon request.",
	"repository": {
		"type": "git",
		"url": "https://github.com/CaptRagnarok/vscode-ragnarok-coder-dark-theme"
	},
	"keywords": [
		"theme",
		"dark theme",
		"ragnarok coding",
		"yellow",
		"dark yellow",
		"yellow blue"
	],

	"type": "dark",
	"colors": {
		"editor.background": "#2c3030",
		"editor.foreground": "#d4d4d4",
		"activityBarBadge.background": "#EBDE01",
		"activityBarBadge.foreground": "#000000",
		"sideBarTitle.foreground": "#bbbbbb",
		"window.activeBorder": "#B8AE00",
		"window.inactiveBorder": "#6B6600",
		"button.background": "#EBDE01F0",
		"button.foreground": "#000000",
		"input.border": "#999100",
		"editorGroup.border": "#EBDE013F",
		"editorGroup.dropBackground":"#EBDE003F",
		"editorCursor.foreground": "#EBDE01",
		"terminal.foreground":"#EBDE01",
		"statusBar.background" : "#6B6600",
    "statusBar.noFolderBackground" : "#212121",
    "statusBar.debuggingBackground": "#263238",
		"statusBar.foreground":"#d4d4d4",
		"panelTitle.activeBorder":"#EBDE01",
		"focusBorder":"#8F8800",
		"widget.shadow":"#EBDE017F",
		"sash.hoverBorder":"#999100",
		"scrollbarSlider.activeBackground":"#EBDF00",
		"scrollbarSlider.hoverBackground":"#999100",
		"scrollbarSlider.background": "#525252",
		"list.activeSelectionBackground":"#FFFFFF28",
		"list.dropBackground":"#EBDE0128",
		"list.hoverBackground":"#EBDE011E",
		"activityBar.background":"#3c4545",
		"sideBar.background":"#353b3b",
		"minimap.findMatchHighlight":"#EBDE0178",
		"minimap.selectionHighlight":"#3baaff32",
		"tab.activeBackground":"#3d3d3d",
		"tab.activeBorderTop":"#999100",
		"editor.findMatchBackground":"#EBDE0178",
		"editor.findMatchHighlightBackground":"#99910078",
		"panel.border":"#6B6600",
		"titleBar.border": "#6B6600",
		"menu.selectionBackground":"#EBDE017F",
		"menu.separatorBackground":"#EBDE017F",
		"settings.checkboxBorder":"#EBDE017F",
		"editor.selectionHighlightBackground":"#FFFFFF32",
		"editor.wordHighlightBackground":"#ff0000"
		

	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#62a348",
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#9bcce8"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color",
				"constant.other.color.rgb-value.hex.css"
			],
			"settings": {
				"foreground": "#B9B600D8",
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.terminator.rule.css",
				"punctuation.separator.key-value.html"
			],
			"settings": {
				"foreground": "#A8A8A8"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"entity.other.attribute-name.pseudo-class.css",
				"punctuation.definition.entity.css",
				"punctuation.separator.list.comma.css",
				"punctuation.separator.key-value.css",
				"entity.other.attribute-name.class.css"
			],
			"settings": {
				"foreground": "#CCBF2C"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution",
